---
import BaseLayout from "../layouts/BaseLayout.astro";
import Navigation from "../components/global/Navigation.astro";
import Privacy from "../components/infopages/Privacy.astro";
import Footer from "../components/global/Footer.astro";

import { AstroSeo } from "@astrolib/seo";
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="Privacy"
  description="A CLI to track the time you spend on projects, with useful reports and analytics"
  canonical="http://usetrak.com"
  openGraph={{
    url: "http://usetrak.com",
    title: "Open Graph Title",
    description: "Open Graph Description",
    images: [
      {
        url: "http://usetrak.com/opengraph/social-og.jpg",
        width: 1200,
        height: 630,
        alt: "Social open graph",
        type: "image/jpeg",
      },
      {
        url: "http://usetrak.com/opengraph/twitter-og.jpg",
        width: 4096,
        height: 4096,
        alt: "Twitter open graph",
        type: "image/jpeg",
      },
      { url: "http://usetrak.com/opengraph/social-og.jpg" },
      { url: "http://usetrak.com/opengraph/twitter-og.jpg" },
    ],
    site_name: "Trak",
  }}
/>
<BaseLayout>
  <Navigation />
  <Privacy />
  <Footer />
</BaseLayout>
